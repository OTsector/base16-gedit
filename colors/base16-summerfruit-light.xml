<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="summerfruit-light" name="Base16 Summerfruit Light" version="1.0">
	<author>Christopher Corley (http://christop.club/)</author>
	
	<!-- Scheme colors -->
	<color name="base00" value="#FFFFFF" />
	<color name="base01" value="#E0E0E0" />
	<color name="base02" value="#D0D0D0" />
	<color name="base03" value="#B0B0B0" />
	<color name="base04" value="#000000" />
	<color name="base05" value="#101010" />
	<color name="base06" value="#151515" />
	<color name="base07" value="#202020" />
	<color name="base08" value="#FF0086" />
	<color name="base09" value="#FD8900" />
	<color name="base0A" value="#ABA800" />
	<color name="base0B" value="#00C918" />
	<color name="base0C" value="#1FAAAA" />
	<color name="base0D" value="#3777E6" />
	<color name="base0E" value="#AD00A1" />
	<color name="base0F" value="#CC6633" />
	
	<style name="text" foreground="base05" background="base00" />
	<style name="selection" background="base02" />
	<style name="cursor" foreground="base05" background="base05" />
	<style name="current-line" foreground="base03" background="base01" />
	<style name="line-numbers" foreground="base04" background="base01" />
	<style name="bracket-match" foreground="base08" background="base00" />
	<style name="search-match" foreground="base02" background="base0A" />
	
	<style name="def:function" foreground="base0D" />
	<style name="def:identifier" foreground="base08" />
	<style name="def:keyword" foreground="base0E" />
	<style name="def:number" foreground="base09" />
	<style name="def:boolean" foreground="base09" />
	<style name="def:string" foreground="base0B" />
	<style name="def:special-char" foreground="base0C" />
	<style name="def:builtin" foreground="base0C" />
	<style name="def:type" foreground="base0A" />
	<style name="def:preprocessor" foreground="" />
	<style name="def:comment" foreground="base03" />
</style-scheme>
